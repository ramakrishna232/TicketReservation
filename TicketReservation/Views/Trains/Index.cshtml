@model IEnumerable<TicketReservation.Models.Train>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Source)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Destination)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrainName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TrainNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACFare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SLFare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SSFare)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ACAvailable)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SLAvailable)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SSAvailable)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Source)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Destination)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrainName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TrainNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACFare)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SLFare)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SSFare)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ACAvailable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SLAvailable)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SSAvailable)
        </td>
       
    </tr>
}

</table>
